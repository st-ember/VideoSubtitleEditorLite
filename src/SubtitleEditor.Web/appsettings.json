{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database": "Error"
    }
  },
  "AllowedHosts": "*",

  "Version": "1.0.38",

  //"Kestrel": {
  //  "Endpoints": {
